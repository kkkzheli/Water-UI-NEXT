{
    //Made By kkkzheli
    //这里是所有自定义功能的修改区
    //字体请写入WaterFont文件夹中的文件名
    //use意思是启用
    //true就是启用, false就是禁用
    //这些自定义功能仅可在标准版V8及以上版本中使用
    //网易请使用smooth字体
    //------------------------------------------
    "$ui_version": "NEXT 1.9",//版本号, 仅限开发者填写
    "$WaterFontType": "OldSmoothFont",//自定义字体, 区分大小写 (OldSmoothFont 圆滑字体 / smooth 默认像素字体 / MinecraftTen 特色像素字体)
    "$use_welcome": true,//欢迎动画
    "$use_doll": true,//常驻纸娃娃
    "$use_hotbar": true,//快捷栏底部黑框及其上方按钮
    "$use_key_display": true,//按键显示
    "$use_hud_info": true,//指南针,钟,手持物品,操作方式
    "$use_potion_list": true,//酿造台里的药水配方列表
    "$use_snow": false,//部分界面的雪花特效 (谨慎开启, 对设备性能要求稍高)
    "$use_container_bottom_blue": false,//容器底部的蓝色渲染
    "$use_container_icon": true,//所有容器的小图标
    "$use_container_paper_doll": true,//所有容器中的纸娃娃
    "$use_container_info": true,//所有容器纸娃娃上方的血量, 饥饿度显示
    "$use_furnace_list": true,//熔炉,火炉,高炉中的配方表
    "$loading_type": "chain",//加载样式 (填写bar/circle/chain三者中任一)
    "$use_clock": false,//启用时钟样式计时器 (谨慎开启, 对设备性能要求较高)
    "$use_gradual_blur_background": true,//模糊度渐变背景 (考虑到设备性能问题, 目前仅对少数界面启用)
    "$debug_mode": false,//调试模式, 在开启状态下您将可以查看实时调试信息 (仅限开发者使用)
    
